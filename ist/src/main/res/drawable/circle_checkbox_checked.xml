<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <corners android:radius="@dimen/_1sdp" />
            <stroke
                android:width="1dp"
                android:color="@color/blue" />
            <solid android:color="@color/blue" />
            <size
                android:width="@dimen/_20sdp"
                android:height="@dimen/_20sdp" />
        </shape>
    </item>

    <item
        android:width="@dimen/_6sdp"
        android:height="@dimen/_2sdp"
        android:left="@dimen/_3sdp"
        android:top="@dimen/_13sdp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>

    <item
        android:width="@dimen/_14sdp"
        android:height="@dimen/_2sdp"
        android:left="@dimen/_5sdp"
        android:top="@dimen/_10sdp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>